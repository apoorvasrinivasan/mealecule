    <relations>
        <relation code="MealeculeQuotientToProduct" localized="false" generate="true" autocreate="true">
            <sourceElement type="MealeculeQuotient" qualifier="mealeculeQuotient" cardinality="one" />
            <targetElement type="Product" qualifier="product" cardinality="many" />
